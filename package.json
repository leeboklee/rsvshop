{
  "name": "rsvshop",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=1024' npx next dev --port 4900",
    "dev:fast": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=2048 --max-semi-space-size=128' npx next dev --port 4900 --turbo",
    "dev:ultra": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=512' npx next dev --port 4900",
    "dev:bg": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=2048' nohup nodemon --config nodemon.json --exec 'npx next dev --port 4900' > dev-server.log 2>&1 &",
    "dev:pm2": "pm2 start npm --name 'rsvshop-dev' -- run dev",
    "dev:pm2:stop": "pm2 stop rsvshop-dev",
    "dev:pm2:restart": "pm2 restart rsvshop-dev",
    "dev:pm2:logs": "pm2 logs rsvshop-dev",
    "dev:pm2:status": "pm2 status",
    "dev:https": "NODE_ENV=wsl HOSTNAME=0.0.0.0 NODE_OPTIONS='--max-old-space-size=2048 --max-semi-space-size=128' npx next dev --port 4901 --experimental-https",
    "dev:both": "concurrently \"npm run dev\" \"npm run dev:https\"",
    "dev:http": "NODE_ENV=wsl HOSTNAME=0.0.0.0 NODE_OPTIONS='--max-old-space-size=2048 --max-semi-space-size=128' npx next dev --port 4900",
    "dev:simple": "NODE_ENV=wsl HOSTNAME=0.0.0.0 NODE_OPTIONS='--max-old-space-size=2048 --max-semi-space-size=128' npx next dev --port 4900",
    "dev:wsl": "NODE_ENV=wsl NODE_OPTIONS='--max-old-space-size=1024' npx next dev --port 4900",
    "dev:customer": "NODE_ENV=wsl NODE_OPTIONS='--max-old-space-size=1024' npx next dev --port 5990",
    "dev:nodemon": "NODE_ENV=wsl NODE_OPTIONS='--max-old-space-size=1024' nodemon --config nodemon.json --exec 'npx next dev --port 4900'",
    "dev:fast": "NODE_ENV=wsl NODE_OPTIONS='--max-old-space-size=1024' npx next dev -p 4900",
    "dev:ultra": "NODE_ENV=wsl NODE_OPTIONS='--max-old-space-size=512' npx next dev -p 4900",
    "dev:compile": "NODE_ENV=wsl NODE_OPTIONS='--max-old-space-size=512 --max-semi-space-size=64' npx next dev -p 4900",
    "dev:fix": "NODE_ENV=wsl HOSTNAME=0.0.0.0 NODE_OPTIONS='--max-old-space-size=2048 --max-semi-space-size=128' npx next dev -p 4900 --experimental-https",
    "dev:optimized": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096 --max-semi-space-size=256' npx next dev --port 4900",
    "fix:all": "bash ./scripts/auto-recovery.sh",
    "fix:server": "bash ./scripts/auto-recovery.sh --server-only",
    "fix:db": "bash ./scripts/check-db-connection.sh",
    "start:safe": "bash ./scripts/start-server-with-db-check.sh",
    "start:auto": "bash ./scripts/start-server-with-db-check.sh",
    "dev:swc": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' npx next dev --port 4900 --swc-minify",
    "dev:parallel": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' npx next dev --port 4900",
    "dev:cache": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' npx next dev --port 4900",
    "db:start": "bash ./scripts/db-control.sh start",
    "db:stop": "bash ./scripts/db-control.sh stop",
    "db:status": "bash ./scripts/db-control.sh status",
    "db:ensure": "bash ./scripts/db-control.sh ensure",
    "db:toggle": "bash ./scripts/db-control.sh toggle",
    "dev:with-db": "npm run db:ensure && npm run dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:customer": "NODE_ENV=wsl npx next start --port 5990",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "node scripts/test-api.js",
    "test:quick": "./scripts/quick-test.sh",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --accept-data-loss",
    "db:backup": "node scripts/auto-backup-scheduler.js",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:all": "rm -rf .next && rm -rf node_modules/.cache && rm -rf logs/* && rm -rf test-results/*",
    "clean:build": "rm -rf .next && rm -rf node_modules/.cache && npm run build",
    "analyze": "ANALYZE=true npm run build"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@pandacss/dev": "^0.54.0",
    "@prisma/client": "^6.13.0",
    "@tosspayments/payment-sdk": "^1.7.0",
    "@types/file-saver": "^2.0.7",
    "adm-zip": "^0.5.16",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "file-saver": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.3",
    "next-connect": "^1.0.0",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.51.1",
    "pngjs": "^7.0.0",
    "react": "18.3.1",
    "react-calendar": "^4.8.0",
    "react-datepicker": "^6.9.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "18.3.1",
    "react-icons": "^5.2.1",
    "winston": "^3.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.4",
    "@playwright/test": "^1.54.1",
    "@types/node": "20.19.9",
    "@types/react": "18.3.3",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "jsdom": "^26.1.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.10",
    "portfinder": "^1.0.37",
    "postcss": "^8.4.38",
    "prisma": "^6.13.0",
    "puppeteer": "^22.15.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  }
}
